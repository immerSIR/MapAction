{
    "name": "map-action-dashboard",
    "version": "1.0.3",
    "private": true,
    "homepage": "/",
    "dependencies": {
        "@babel/plugin-proposal-optional-chaining": "^7.21.0",
        "@chakra-ui/icons": "^1.1.7",
        "@chakra-ui/react": "1.8.8",
        "@chakra-ui/system": "^1.12.1",
        "@chakra-ui/theme-tools": "^1.3.6",
        "@emotion/cache": "^11.4.0",
        "@emotion/react": "^11.7.1",
        "@emotion/styled": "^11.6.0",
        "@fontsource/open-sans": "^4.5.0",
        "@fontsource/raleway": "^4.5.0",
        "@fontsource/roboto": "^4.5.0",
        "ajv": "^8.17.1",
        "ajv-keywords": "^5.1.0",
        "apexcharts": "^3.27.3",
        "axios": "^1.7.2",
        "chart.js": "^4.4.4",
        "classnames": "2.3.1",
        "date-fns": "^3.6.0",
        "dompurify": "^3.1.6",
        "framer-motion": "4.1.17",
        "leaflet": "^1.9.4",
        "marked": "^14.1.2",
        "match-sorter": "6.3.0",
        "moment": "^2.30.1",
        "nouislider": "15.0.0",
        "react": "^18.2.0",
        "react-apexcharts": "^1.3.9",
        "react-big-calendar": "0.33.2",
        "react-bootstrap-sweetalert": "5.2.0",
        "react-custom-scrollbars": "^4.2.1",
        "react-date-range": "^2.0.1",
        "react-datetime": "3.0.4",
        "react-dom": "^18.2.0",
        "react-github-btn": "^1.2.1",
        "react-icons": "^4.2.0",
        "react-jvectormap": "0.0.16",
        "react-leaflet": "^4.2.1",
        "react-markdown": "^9.0.1",
        "react-router-dom": "5.2.0",
        "react-scripts": "^5.0.1",
        "react-select": "^5.8.0",
        "react-slick": "^0.30.2",
        "react-swipeable-views": "0.14.0",
        "react-table": "7.7.0",
        "react-tagsinput": "3.19.0",
        "sass": "^1.56.1",
        "slick-carousel": "^1.8.1",
        "stylis": "^4.0.10",
        "stylis-plugin-rtl": "2.0.2",
        "sweetalert2": "^11.6.13",
        "video-react": "^0.16.0"
    },
    "resolutions": {
        "react-error-overlay": "6.0.9"
    },
    "scripts": {
        "start": "NODE_NO_WARNINGS=1 NODE_OPTIONS=--openssl-legacy-provider react-scripts start",
        "build": "NODE_NO_WARNINGS=1 NODE_OPTIONS=--openssl-legacy-provider react-scripts build && gulp licenses",
        "test": "NODE_NO_WARNINGS=1 NODE_OPTIONS=--openssl-legacy-provider react-scripts test --env=jsdom",
        "eject": "react-scripts eject",
        "deploy": "npm run build",
        "lint:check": "eslint . --ext=js,jsx;  exit 0",
        "lint:fix": "eslint . --ext=js,jsx --fix;  exit 0",
        "install:clean": "rm -rf node_modules/ && rm -rf package-lock.json && npm install && npm start",
        "test:coverage": "react-scripts test --coverage --watchAll=false"
    },
    "optionalDependencies": {
        "@babel/core": "7.14.0",
        "typescript": "4.2.4"
    },
    "devDependencies": {
        "@babel/plugin-transform-react-jsx-source": "^7.14.5",
        "@testing-library/dom": "^10.4.0",
        "@testing-library/jest-dom": "^6.6.3",
        "@testing-library/react": "^16.2.0",
        "@testing-library/user-event": "^14.6.1",
        "eslint-config-prettier": "8.3.0",
        "eslint-plugin-prettier": "3.4.0",
        "gulp": "^5.0.0",
        "gulp-append-prepend": "1.0.9",
        "jest": "^29.7.0",
        "jest-environment-jsdom": "^29.7.0",
        "prettier": "2.2.1"
    },
    "browserslist": {
        "production": [
            ">0.2%",
            "not dead",
            "not op_mini all"
        ],
        "development": [
            "last 1 chrome version",
            "last 1 firefox version",
            "last 1 safari version"
        ]
    },
    "proxy": "https://api.map-action.com",
    "jest": {
        
        "testMatch": [
          "<rootDir>/src/__tests__/**/*.{js,jsx,ts,tsx}"
        ],
        "collectCoverageFrom": [
          "src/**/*.{js,jsx,ts,tsx}",
          "!src/**/*.d.ts",
          "!src/index.js",
          "!src/serviceWorker.js"
        ],
        "moduleNameMapper": {
            "^axios$": "<rootDir>/node_modules/axios/dist/axios.js",
            "\\.(css|less|scss|sass)$": "identity-obj-proxy",
            "\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/__mocks__/fileMock.js",
            "^Fonctions/(.*)$": "<rootDir>/src/Fonctions/$1",
            "^variables/(.*)$": "<rootDir>/src/variables/$1",
            "^react-leaflet$": "<rootDir>/node_modules/react-leaflet/lib/index.js"
        },
        "transformIgnorePatterns": [
         "node_modules/(?!(@?react-leaflet|leaflet|react-markdown|@react-leaflet|react-icons|@chakra-ui|@emotion|react-apexcharts|axios|@babel|@jest)/)"
        ],

        "transform": {
          "^.+\\.(js|jsx|ts|tsx)$": "babel-jest",
          "node_modules/react-leaflet/.+\\.(js|jsx)$": "babel-jest"
        },
        "clearMocks": true,
        "resetMocks": true,
        "restoreMocks": true,
        "coverageReporters": [
            "text",
            "lcov",
            "html",
            "text-summary",
            "json-summary"

        ]
        
    },
    "babel": {
      "presets": [
          "@babel/preset-env",
          ["@babel/preset-react", { "runtime": "automatic" }]
      ],
      "ignore": [],
      "overrides": [
          {
          "include": "./node_modules/react-leaflet",
          "presets": ["@babel/preset-env"]
          }
      ]
    }

}
